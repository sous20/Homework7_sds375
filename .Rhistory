senic = read.table("senic.txt", header = TRUE)
senic$Region = as.factor(senic$Region)
model = lm(LengthOfStay ~ InfectionRisk + Age + log(NumberBeds) + Region, data = senic)
par(mfrow = c(3,2), mar = c(3,3,2,1))
plot(senic$InfectionRisk, model$residuals, xlab = "Infection Risk", ylab = "residuals")
abline(0,0, col = "blue")
plot(senic$Age, model$residuals, xlab = "Age", ylab = "residuals")
abline(0,0, col = "blue")
plot(log(senic$NumberBeds), model$residuals, xlab = "log(NumberBeds)", ylab = "residuals")
abline(0,0, col = "blue")
plot(senic$Region, model$residuals, xlab = "Region", ylab = "residuals")
plot(model$fitted.values, model$residuals, xlab = "fitted", ylab = "residuals")
abline(0,0, col = "blue")
par(mfrow=c(1,2))
hist(model$residuals, freq = FALSE, xlab = "Residual", main = "")
curve(dnorm(x, mean = 0, sd = sd(model$residuals)), add = TRUE, col = "blue")
qqnorm(model$residuals, pch=16, cex.axis=1.3, cex.lab=1.3)
qqline(model$residuals)
cook_d = cooks.distance(model)
